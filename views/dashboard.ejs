<div>
    <a href="/add-topic" class="btn btn-primary">
        Add Topic 
    </a>
</div>
<br>

<div id="topic-list">
    <!-- need to fetch all topics which username  is same-->
    <h4>Your all topics visble Here: </h4>
    <ul class="list-group list-group-checkable d-grid gap-2 border-0 w-auto bg-secodary">
        <% for(let topic of topics) { %>
            <li class="topic ">
                 
                 <label class="list-group-item rounded-3 py-3" for="listGroupCheckableRadios3">
                    <a href="/topic-details/<%= topic._id %>"><%=topic.topicName %> </a>
                    <h6>understand : <%= topic.percentage %> %</h6>
                </label>
            </li>
        <% } %>
    </ul>
    
</div>